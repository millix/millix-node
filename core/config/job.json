{
  "version": 6,
  "processor_list": {
    "localhost": {
      "instances": 2
    },
    "localhost_wallet": {
      "instances": 2
    },
    "localhost_watchdog": {
      "instances": 2
    },
    "localhost_network": {
      "instances": 1
    }
  },
  "object_list": {
    "transaction": {
      "object_type": "table"
    },
    "transaction_input": {
      "object_type": "table"
    },
    "transaction_output": {
      "object_type": "table"
    },
    "transaction_parent": {
      "object_type": "table"
    },
    "transaction_signature": {
      "object_type": "table"
    }
  },
  "job_list": {
    "peer_rotation": {
      "type": "function",
      "group": "node",
      "processor": "localhost_network",
      "payload": {
        "module": "peer",
        "function_name": "_doPeerRotation"
      },
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 10000,
        "enable": true
      }
    },
    "node_list": {
      "type": "function",
      "group": "network",
      "processor": "localhost",
      "payload": {
        "module": "peer",
        "function_name": "sendNodeList"
      },
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 60000,
        "enable": true
      }
    },
    "transaction_validation": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost",
      "payload": {
        "module": "wallet-transaction-consensus",
        "function_name": "doValidateTransaction"
      },
      "object_list": [
        "transaction",
        "transaction_input",
        "transaction_output"
      ],
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 1000,
        "enable": true
      }
    },
    "transaction_spend_sync": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost_wallet",
      "payload": {
        "module": "wallet-sync",
        "function_name": "_doSyncTransactionSpend"
      },
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 2000,
        "enable": true
      }
    },
    "consensus_watchdog_validation": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost_watchdog",
      "payload": {
        "module": "wallet-transaction-consensus",
        "function_name": "doConsensusTransactionValidationWatchDog"
      },
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 10000,
        "enable": true
      }
    },
    "wallet_inspect": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost_wallet",
      "payload": {
        "module": "wallet",
        "function_name": "_doStateInspector"
      },
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 5000,
        "enable": true
      }
    },
    "wallet_retry_validation_update": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost_wallet",
      "payload": {
        "module": "wallet",
        "function_name": "_doUpdateRetryTransactionValidation"
      },
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 10000,
        "enable": true
      }
    },
    "shard_zero_pruning": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost_wallet",
      "payload": {
        "module": "wallet",
        "function_name": "_doShardZeroPruning"
      },
      "object_list": [
        "transaction",
        "transaction_input",
        "transaction_output",
        "transaction_signature",
        "transaction_parent"
      ],
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 5000,
        "enable": true
      }
    },
    "dag_progress": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost",
      "payload": {
        "module": "wallet",
        "function_name": "_doDAGProgress"
      },
      "object_list": [
        "transaction",
        "transaction_input",
        "transaction_output"
      ],
      "priority": 1,
      "option_list": {
        "run_always": 1,
        "run_delay": 10000,
        "enable": false
      }
    },
    "transaction_output_expiration": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost_wallet",
      "payload": {
        "module": "wallet",
        "function_name": "_doTransactionOutputExpiration"
      },
      "object_list": [
        "transaction",
        "transaction_output"
      ],
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 10000,
        "enable": true
      }
    },
    "node_attribute_update": {
      "type": "function",
      "group": "wallet",
      "processor": "localhost_wallet",
      "payload": {
        "module": "wallet",
        "function_name": "_doUpdateNodeAttribute"
      },
      "object_list": [],
      "priority": 0,
      "option_list": {
        "run_always": 1,
        "run_delay": 30000,
        "enable": true
      }
    }
  }
}
